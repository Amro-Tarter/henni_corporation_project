rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
     match /elemental_projects/{document=**} {
      allow read, write: if true;  
    }
  }
}
// AFTER UPDATING RUN THIS: firebase deploy --only firestore:rules
